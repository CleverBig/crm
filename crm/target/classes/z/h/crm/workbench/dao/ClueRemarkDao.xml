<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="z.h.crm.workbench.dao.ClueRemarkDao">

    <select id="selectRemarkByClueId" resultType="ClueRemark">
        select * from tbl_clue_remark where clueId = #{cid}
    </select>

    <delete id="delete">
        delete from tbl_clue_remark where cludId = #{cid}
    </delete>

</mapper>

































